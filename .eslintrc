{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
    "modules": true,
    "jsx": true
  },
  "rules": {
    "strict": 0,
    "indent": [2, 2],
    "quotes": [2, "single"],
    "curly": false,
    "new-cap": true,
    "no-shadow": true,
    "no-unused-vars": true,
    "no-underscore-dangle": false
  }
}